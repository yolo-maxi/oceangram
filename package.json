{
  "name": "oceangram",
  "displayName": "Oceangram",
  "description": "Developer cockpit â€” Comms, Kanban, Resources, and Agent Status in one sidebar",
  "version": "0.1.0",
  "publisher": "oceangram",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "oceangram",
          "title": "Oceangram",
          "icon": "media/oceangram.svg"
        }
      ]
    },
    "views": {
      "oceangram": [
        {
          "type": "webview",
          "id": "oceangram.kanban",
          "name": "Kanban"
        },
        {
          "type": "webview",
          "id": "oceangram.resources",
          "name": "Resources"
        },
        {
          "type": "webview",
          "id": "oceangram.agentStatus",
          "name": "Agent Status"
        }
      ]
    },
    "commands": [
      {
        "command": "oceangram.refreshAll",
        "title": "Oceangram: Refresh All Panels"
      },
      {
        "command": "oceangram.openComms",
        "title": "Oceangram: Open Comms",
        "icon": "ðŸ’¬"
      }
    ],
    "keybindings": [
      {
        "command": "oceangram.openComms",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "telegram": "^2.26.22"
  }
}
