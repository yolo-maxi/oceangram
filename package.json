{
  "name": "oceangram",
  "displayName": "Oceangram",
  "description": "Developer cockpit â€” Comms, Kanban, Resources, and Agent Status in one sidebar",
  "version": "0.1.0",
  "publisher": "oceangram",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oceangram.openComms",
        "title": "Oceangram: Open Comms"
      },
      {
        "command": "oceangram.openKanban",
        "title": "Oceangram: Open Kanban"
      },
      {
        "command": "oceangram.openResources",
        "title": "Oceangram: Open Resources"
      },
      {
        "command": "oceangram.openAgent",
        "title": "Oceangram: Open Agent Status"
      }
    ],
    "keybindings": [
      {
        "command": "oceangram.openComms",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1"
      },
      {
        "command": "oceangram.openKanban",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2"
      },
      {
        "command": "oceangram.openResources",
        "key": "ctrl+shift+3",
        "mac": "cmd+shift+3"
      },
      {
        "command": "oceangram.openAgent",
        "key": "ctrl+shift+4",
        "mac": "cmd+shift+4"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "telegram": "^2.26.22"
  }
}
